{
    "title": "Athena ‚Äî Service Health & Infrastructure",
    "uid": "athena-infra-v1",
    "tags": [
        "athena",
        "infrastructure",
        "jvm"
    ],
    "timezone": "Africa/Nairobi",
    "refresh": "30s",
    "time": {
        "from": "now-3h",
        "to": "now"
    },
    "schemaVersion": 38,
    "panels": [
        {
            "type": "row",
            "id": 1,
            "title": "üü¢ Java Service",
            "gridPos": {
                "x": 0,
                "y": 0,
                "w": 24,
                "h": 1
            }
        },
        {
            "id": 2,
            "title": "HTTP Request Rate (req/s)",
            "type": "timeseries",
            "gridPos": {
                "x": 0,
                "y": 1,
                "w": 12,
                "h": 8
            },
            "datasource": {
                "type": "prometheus",
                "uid": "athena-prometheus"
            },
            "targets": [
                {
                    "expr": "rate(http_server_requests_seconds_count{job=\"athena-java-service\"}[1m])",
                    "legendFormat": "{{method}} {{uri}} {{status}}"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "reqps",
                    "color": {
                        "mode": "palette-classic"
                    }
                }
            }
        },
        {
            "id": 3,
            "title": "HTTP Latency p50/p90/p99 (ms)",
            "type": "timeseries",
            "gridPos": {
                "x": 12,
                "y": 1,
                "w": 12,
                "h": 8
            },
            "datasource": {
                "type": "prometheus",
                "uid": "athena-prometheus"
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.50, rate(http_server_requests_seconds_bucket{job=\"athena-java-service\"}[1m])) * 1000",
                    "legendFormat": "p50"
                },
                {
                    "expr": "histogram_quantile(0.90, rate(http_server_requests_seconds_bucket{job=\"athena-java-service\"}[1m])) * 1000",
                    "legendFormat": "p90"
                },
                {
                    "expr": "histogram_quantile(0.99, rate(http_server_requests_seconds_bucket{job=\"athena-java-service\"}[1m])) * 1000",
                    "legendFormat": "p99"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "ms",
                    "color": {
                        "mode": "palette-classic"
                    }
                }
            }
        },
        {
            "id": 4,
